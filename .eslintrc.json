{
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-useless-escape": 0,
      "no-console": 0,
      "func-names": 0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
      "class-methods-use-this": 0,
      "consistent-return": 0
    }
}
